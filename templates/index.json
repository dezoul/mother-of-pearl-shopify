    "247bdb45-51ad-4b49-8988-da1256ed393d": {
      "type": "banner-alternativev2",
      "blocks": {
        "template--20007614153008__247bdb45-51ad-4b49-8988-da1256ed393d-1689493192c9b220c8-0": {
