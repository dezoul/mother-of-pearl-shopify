{
  "sections": {
    "main": {
      "type": "cart__main",
      "custom_css": [
        ".color__border-divider-1 {border-color: #e0d3b4;}",
        ".cart-item *,input[type=\"checkbox\"] {background: none !important; border-color: #e0d3b4;}",
        ".cart-item .item-quantity {border: none; color: #20242b;}",
        ".cart-item .item-quantity *,.cart-item input,.cart-item .btn--plain * {color: #20242b; opacity: 1;}",
        "img {mix-blend-mode: multiply;}",
        ".aspect-w-1 {background-color: #faf6f1;}",
        ".component-badge {color: inherit; background-color: #f8f4eb;}"
      ],
      "settings": {
        "spacing_top": 100,
        "spacing_bottom": 100,
        "page_color_scheme": "color__bg-secondary color__secondary",
        "color_border": "color__border-divider-1",
        "sidebar_color_scheme": "color__bg-secondary color__secondary",
        "main_disclaimer": "<p>We'll review your order and personally get in touch to confirm stock availabilty, custom finishes and project timelines. <\/p>",
        "empty_button_label": "Shop all",
        "empty_button_url": "\/collections\/all",
        "enable_cart_note": true,
        "enable_dynamic_checkout": true
      }
    }
  },
  "order": [
    "main"
  ]
}
